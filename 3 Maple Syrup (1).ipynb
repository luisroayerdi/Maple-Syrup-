    "import string\n",
    "with open(\"level 3.txt\") as f:\n",
    "    txt = f.read()\n",
    "alphabet = string.ascii_lowercase \n",
    "shifted_alphabet = alphabet[16:] + alphabet[:16]\n",
    "table = str.maketrans(alphabet, shifted_alphabet)\n",
    "print(txt.translate(table))"
